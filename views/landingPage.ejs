    <section class="enquireUs">
      <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
      <div class="center hideform enquireBack">
        <button id="close" style="position: absolute; right: 19px;
            bottom: 377px; background: red; padding-left: 6px; padding-right: 6px; color: white;">X</button>
        <form action="/enquire/store" method="POST" style="position:absolute; right: 0.8rem;bottom: 1rem;color: black;"
          class="enquireForm">
          Name:<br>
          <input type="text" placeholder="Your name" id="username" name="username" value="<%=username %>">
          <br>
          Phone no:<br>
          <input type="tel" placeholder="Phone no." name="phone" id="phone" value="<%=phone %>" maxlength="10"
            pattern="\d{10}" title="Please enter exactly 10 digits">
          <br>
          <!-- E-mail:<br>
            <input type="email" placeholder="E-mail" name="email" id="email" value="<%=email %>">
            <br> -->
          Age-group:<br>
          <label class="container">18-25
            <input type="radio" name="age" value="18-25" id="radio" checked>
          </label>
          <label class="container">26-30 <input type="radio" value="26-30" name="age" id="radio">
          </label>
          <label class="container">30+ <input type="radio" value="30+" name="age" id="radio">

          </label> <br>
          Message:<br>
          <textarea name="message" id="message" cols="30" rows="10" style="resize: none; overflow-y: scroll;
            height: 125px;"></textarea>
          <br>
          <input type="submit" value="Enquire" style="background: green; color: white; margin-top: 4px;">
        </form>

      </div>
      <% if(errors !=null && errors.length>0){ %>
        <div style="position: relative;bottom: -9px;
          right: 10px;" class="formError">
          <ul class="list-group">
            <% for(var i=0;i<errors.length; i++){ %>
              <li class="list-group-item list-group-item-danger" style="background-color:red;color: white;">
                <%=errors[i] %>
              </li>
              <% } %>
          </ul>
        </div>
        <% } %>
          <button id="show"><img src="/images/enquire.png" /> <br>Enquire</button>
          <script>
            $('#show').on('click', function () {
              $('.center').show();
              $('.formError').hide();
              $(this).hide();
            })

            $('#close').on('click', function () {
              $('.center').hide();
              $('#show').show();
            })
          </script>
    </section>

